{
  "name": "antd-style",
  "version": "3.0.0-alpha.17",
  "description": "a css-in-js solution for application combine with antd v5 token system and emotion",
  "keywords": [
    "css-in-js",
    "cssinjs",
    "emtion",
    "token",
    "design token",
    "token-system",
    "antd"
  ],
  "homepage": "https://github.com/arvinxx/antd-style",
  "bugs": {
    "url": "https://github.com/arvinxx/antd-style"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arvinxx/antd-style.git"
  },
  "license": "MIT",
  "author": "arvinxx(arvinx@foxmail.com)",
  "sideEffects": false,
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "es"
  ],
  "scripts": {
    "build": "father build",
    "build:watch": "father dev",
    "ci": "npm run lint && npm run type-check",
    "clean": "rm -rf es lib dist coverage .dumi/tmp .eslintcache",
    "dev": "dumi dev",
    "docs:build": "dumi build",
    "doctor": "father doctor",
    "lint": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "prepare": "husky install && npm run setup",
    "prepublishOnly": "father doctor && npm run build",
    "prettier": "prettier -c --write \"**/**\"",
    "release": "semantic-release",
    "setup": "dumi setup",
    "start": "dumi dev",
    "test": "jest --passWithNoTests",
    "test:coverage": "jest --coverage  --passWithNoTests",
    "test:update": "jest --update-snapshot",
    "type-check": "tsc -p tsconfig-check.json"
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.20",
    "@emotion/css": "^11",
    "@emotion/react": "^11",
    "@emotion/serialize": "^1",
    "@emotion/styled": "^11",
    "use-merge-value": "^1"
  },
  "devDependencies": {
    "@ant-design/icons": "^4",
    "@ant-design/pro-components": "^2",
    "@commitlint/cli": "^17",
    "@emotion/jest": "^11",
    "@testing-library/jest-dom": "^5",
    "@testing-library/react": "^13",
    "@types/jest": "^27",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/testing-library__jest-dom": "^5",
    "@umijs/lint": "^4",
    "@umijs/test": "^4",
    "antd": "^5",
    "classnames": "^2",
    "commitlint": "^17",
    "commitlint-config-gitmoji": "^2",
    "concurrently": "^7",
    "cross-env": "^7",
    "dumi": "^2",
    "eslint": "^8",
    "father": "^4",
    "husky": "^8",
    "jest": "^29",
    "jest-environment-jsdom": "^29",
    "lint-staged": "^13",
    "prettier": "^2",
    "prettier-plugin-organize-imports": "^3",
    "prettier-plugin-packagejson": "^2",
    "react": "^18",
    "react-dom": "^18",
    "react-layout-kit": "^1",
    "semantic-release": "^19",
    "semantic-release-config-gitmoji": "^1",
    "stylelint": "^14",
    "ts-jest": "^29",
    "ts-node": "^10",
    "typescript": "^4",
    "zustand": "^4"
  },
  "peerDependencies": {
    "antd": "^5",
    "react": ">=18"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
